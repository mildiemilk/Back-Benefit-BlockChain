{
  "presets": ["es2015", "stage-3"],
  "extends": [
    "eslint:recommended",
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "spread": true,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "quotes": 0,
    "indent": ["error", 2, {"SwitchCase": 1}],
    "no-console": 0,
    "no-debugger": 0,
    "semi": [1, "always"],
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "no-underscore-dangle": 0,
    "no-alert": 0,
    "no-lone-blocks": 0,
    "no-unused-vars": [1, {
      "vars": "all",
      "args": "after-used"
    }]
  }
}
